<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Ex1_RequestDispatcher</display-name>
  <servlet>
  <servlet-name>RequestDisp</servlet-name>
  <servlet-class>ReqDisp.RequestDisp</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>Display</servlet-name>
  <servlet-class>ReqDisp.Display</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>RequestDisp</servlet-name>
  <url-pattern>/RequestDisp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>Display</servlet-name>
  <url-pattern>/Display</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>